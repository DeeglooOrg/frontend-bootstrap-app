{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    }
  },
  "env": {
    "es6": true
  },
  "extends": [
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-var": "error"
  }
}